<!DOCTYPE html>
<html>
  <head>
    <title>ReactRails5</title>
    <%= csrf_meta_tags %>

    <%= action_cable_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <header>
      <nav id="navbar"></nav>
      <%= javascript_pack_tag 'navbar-component' %>
    </header>
    <div class="container">
      <p id="notice"><%= notice %></p>

      <%= yield %>
    </div>
  </body>
</html>
